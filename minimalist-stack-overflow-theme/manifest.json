{
    "name": "Minimalist Stack Overflow",
    "description": "A minimalist theme for Stack Overflow",
    "manifest_version": 2,
    "version": "1.0.0",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon_16.png"
    },
    "content_scripts": [
        {
            "css": [
                "style.css"
            ],
            "js": [
                "script.js"
            ],
            "matches": [
                "https://stackoverflow.com/questions*"
            ]
        }
    ]
}