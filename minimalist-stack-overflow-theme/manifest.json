{
    "name": "Minimalist Stack Overflow",
    "description": "A minimalist theme for Stack Overflow",
    "manifest_version": 2,
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "css": [
                "style.css"
            ],
            "js": [
                "script.js"
            ],
            "matches": [
                "https://stackoverflow.com/questions*"
            ]
        }
    ]
}