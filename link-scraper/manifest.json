{
    "manifest_version": 2,
    "name": "Link Scraper",
    "description": "Scrape all the links on a web page",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}