{
    "manifest_version": 2,
    "name": "Link Scraper",
    "description": "Scrape all the links on a web page",
    "version": "1.0.0",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon_16.png"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://*.reddit.com/*"
            ]
        }
    ]
}